 A   175              ;       b       monitor_entry
 A   176              
 A   177              ;
 A   178              ; Load RSP (AD6620) Registers from Table
 A   179              ;
 A   180              ; 23 Dec 2009 took out most writes to terminal (msgout, dis4hex, asx)
 A   181              ;
 A   182              ; This code was taken directly from the "rspmod" routine used in the
 A   183              ; Dartmouth Monitor. Instead of having the user enter the data words
 A   184              ; or receiving them from a "script" file, this routine looks at a
 A   185              ; table of words in memory, reads them, and transfers them to the
 A   186              ; AD6620. Used to load control bytes and filter coefficients.
 A   187              ;
 A   188              ; Table entry format:
 A   189              ;
 A   190              ; rsp_table:
 A   191              ;       .word   AmLah, r4r3h, r2r1h, r0xxh;
 A   192              ;       .word   (more 4-word entries)
 A   193              ;       .word   0FFFFh  ; End of table
 A   194              ;
 A   195              ; 4 words:
 A   196              ; AAaah = AD6620 internal address, 0000h to 030Dh, or FFFFh to terminate.
 A   197              ;         AMR = Ma Address mode register
 A   198              ;         LAR = La Lower address register
 A   199              ; r4r3h, r2r1h, r0xxh = data bytes, packed into words, MS, to LS. Bottom byte
 A   200              ;         of 3rd word not used (xx). Data is treated a 40 bits for all
 A   201              ;         AD6620 registers. Not the most compact arrangement for storage,
 A   202              ;         but readable- and it can be edited directly from monitor scripts.
 A   203              ;
 A   204              ;         DR4 = r4
 A   205              ;         DR3 = r3
 A   206              ;         DR2 = r2
 A   207              ;         DR1 = r1
 A   208              ;         DR0 = r0 
 A   209              ;
 A   210              ; Uses:
 A   211              ; A:   Holds table pointer upon entry 
 A   212              ; B:   Working register
 A   213              ; AR0: I/O address
 A   214              ; AR2: Table index
 A   215              ; AR3: Holds data to send to or read from I/O port
 A   216              ;
 A   217        FFFF  table_end .set 0FFFFh           ; End-of-table definition
 A   218              
 A   219 00024c       rsp_setup:                      ; Enter with table starting address in A
 A   220 00024c 8812          stlm    A,AR2           ; Save to AR2 for later use
 A   221 00024d F495          nop                     ; Necessary for loop to execute properly (?!&)
 A   222 00024e F495          nop                     ; Necessary for loop to execute properly (?!&)
 A   223              ;       ld      #ad6620_msg1,A  ; Tell operator what is happening
 A   224              ;       call    msgout  
 A   225              
